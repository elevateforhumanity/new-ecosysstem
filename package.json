{
  "name": "elevateforhumanity",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": "20.17.0"
  },
  "scripts": {
    "sitemaps:generate": "node scripts/generate-sitemaps.js",
    "sitemaps:ping": "node scripts/ping-sitemaps.js",
    "sitemaps:submit": "npm run sitemaps:generate && npm run sitemaps:ping",
    "build:sitemaps": "node scripts/generate-sitemaps.js",
    "google:apply": "node scripts/apply-google-config.js",
    "dev": "vite",
    "prebuild": "node scripts/validate-env.js",
  "build": "vite build && npm run seo:polish",
    "preview": "vite preview --host",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier -w .",
    "env:check": "node scripts/validate-env.js",
    "size:check": "node scripts/size-check.js",
    "ci:lh": "lhci autorun --config=./lighthouserc.json",
    "logs:tail": "tail -f logs/app-$(date +%F).log || true",
    "logs:rotate": "node scripts/log-rotate.js",
  "sentry:release": "bash scripts/sentry-release.sh",
  "seo:polish": "node scripts/fix-site.mjs",
  "seo:meta": "node enhanced-meta-tags-updater.js",
  "seo:meta:all": "SEO_ALL=true node enhanced-meta-tags-updater.js",
  "engagement:inject": "node scripts/inject-engagement.js",
  "brand:scrub": "node scripts/scrub-brand-mentions.js",
  "site:enhance": "npm run seo:meta:all && npm run engagement:inject && npm run brand:scrub"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
  "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.6.0",
  "@lhci/cli": "0.14.0",
    "autoprefixer": "10.4.20",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jsdom": "26.1.0",
    "prettier": "3.2.0",
    "rollup-plugin-visualizer": "5.12.0",
  "supertest": "7.1.3",
  "@types/supertest": "2.0.16",
    "tailwindcss": "3.4.10",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "5.0.0",
    "vitest": "1.6.0",
    "zod": "3.23.8"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "@supabase/supabase-js": "2.45.0",
    "compression": "1.7.4",
    "express": "5.1.0",
    "express-rate-limit": "7.2.0",
    "helmet": "7.1.0",
    "node-cron": "4.2.1",
    "pdfkit": "0.15.0",
    "pino": "9.0.0",
    "pino-http": "10.0.0",
    "prisma": "5.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "6.23.1",
    "stripe": "14.0.0"
  ,"fast-glob": "3.3.2",
  "cheerio": "1.0.0",
  "xmlbuilder2": "3.1.1"
  },
  "keywords": [],
  "packageManager": "npm@10.0.0"
}
