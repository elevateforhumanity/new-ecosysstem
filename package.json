{
  "name": "elevateforhumanity",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.17.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:partial": "node scripts/prepare-deploy.mjs",
    "build:production": "node scripts/validate-env.js && npm run build:partial",
    "preview": "vite preview --host",
    "ci:changed": "node scripts/changed-paths.mjs",
    "ci:ignore": "node scripts/ignore-if-no-app-change.mjs",
    "postbuild": "node scripts/postbuild-sitemaps.mjs",
    "verify:seo": "node scripts/verify-seo.mjs",
    "sitemap": "node scripts/generate-sitemap.mjs",
    "sitemap:chunk": "node scripts/sitemap-chunk.js",
    "export:cap": "./infra/scripts/export-cap.sh",
    "optimize-images": "node scripts/optimize-images.js",
    "optimize:images": "node infra/scripts/image-optimizer.js",
    "crawl": "node scripts/crawl-site.mjs",
    "crawl:local": "node scripts/crawl-site.mjs --base=http://localhost:8000 --max=100",
    "crawl:prod": "node infra/scripts/crawl-site.mjs --base=https://elevateforhumanity.org --max=10000",
    "verify:redirects": "node scripts/verify-redirects.mjs",
    "verify:redirects:local": "node infra/scripts/verify-redirects.mjs --base=http://localhost:8000",
    "verify:redirects:prod": "node infra/scripts/verify-redirects.mjs --base=https://elevateforhumanity.org",
    "performance": "node scripts/performance-optimizer.js",
    "build:complete": "npm run optimize-images && npm run sitemap && npm run performance",
    "build:optimized": "npm run performance && npm run sitemap",
    "sitemaps:generate": "node scripts/generate-sitemaps.js",
    "sitemaps:ping": "node scripts/ping-sitemaps.js",
    "sitemaps:submit": "npm run sitemaps:generate && npm run sitemaps:ping",
    "build:sitemaps": "node scripts/generate-sitemaps.js",
    "build:deploy": "node scripts/build-deploy.cjs",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier -w .",
    "env:check": "node scripts/validate-env.js",
    "size:check": "node scripts/size-check.js",
    "ci:lh": "lhci autorun --config=./lighthouserc.json",
    "logs:tail": "tail -f logs/app-$(date +%F).log || true",
    "logs:rotate": "node scripts/log-rotate.js",
    "sentry:release": "bash scripts/sentry-release.sh",
    "seo:polish": "node scripts/fix-site.mjs",
    "start": "node server.js",
    "deploy": "./scripts/auto-deploy.sh",
    "deploy:watch": "./scripts/watch-and-deploy.sh",
    "serve-static": "python3 -m http.server 8080",
    "routes": "node scripts/extract-routes.mjs",
    "sitemap:routes": "node scripts/generate-sitemap.mjs --routes=scripts/routes.json --base=https://www.elevateforhumanity.org --out=public",
    "sitemap:crawl": "node scripts/generate-sitemap.mjs --seed=https://www.elevateforhumanity.org --out=public --max=120000 --concurrency=8",
    "sitemap:gitpod": "node scripts/generate-gitpod-sitemap.mjs",
    "build:marketing": "echo 'Marketing site is static'",
    "build:programs": "echo 'Programs site is static'",
    "build:blog": "echo 'Blog site is static'",
    "crawl:marketing": "node infra/scripts/crawl-site.mjs --base=http://localhost:8000 --max=100",
    "reorg": "node infra/scripts/repo-reorg.mjs",
    "sitemaps:tiny": "node scripts/sitemap-partitioner.mjs --base=https://www.elevateforhumanity.org --in=all-urls.txt --out=sites/marketing/sitemaps --chunk=5000",
    "robots": "node -e \"require('fs').writeFileSync('sites/marketing/robots.txt', 'User-agent: *\\nAllow: /\\nSitemap: https://www.elevateforhumanity.org/sitemap_index.xml\\n')\""
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "devDependencies": {
    "@lhci/cli": "0.14.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "5.3.3",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "1.6.0",
    "cheerio": "^1.1.0",
    "cypress": "^15.2.0",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "fast-xml-parser": "^5.2.5",
    "jest": "^30.1.3",
    "jsdom": "26.1.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.55.0",
    "prettier": "3.2.0",
    "robots-parser": "^3.0.1",
    "rollup-plugin-visualizer": "5.12.0",
    "supertest": "7.1.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "5.0.0",
    "vitest": "1.6.0",
    "zod": "3.23.8"
  },
  "dependencies": {
    "@prisma/client": "5.18.0",
    "@supabase/supabase-js": "2.45.0",
    "aos": "^2.3.4",
    "compression": "1.7.4",
    "express": "5.1.0",
    "express-rate-limit": "7.2.0",
    "fast-glob": "3.3.2",
    "framer-motion": "^12.23.12",
    "glob": "^11.0.3",
    "gsap": "^3.13.0",
    "helmet": "7.1.0",
    "lottie-web": "^5.13.0",
    "node-cron": "4.2.1",
    "nodemailer": "^7.0.6",
    "pdfkit": "0.15.0",
    "pino": "9.0.0",
    "pino-http": "10.0.0",
    "prisma": "5.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet-async": "2.0.5",
    "react-router-dom": "6.23.1",
    "stripe": "^14.0.0",
    "twilio": "^5.9.0",
    "xmlbuilder2": "3.1.1"
  },
  "keywords": [],
  "packageManager": "npm@10.0.0"
}