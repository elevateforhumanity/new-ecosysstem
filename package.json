{
  "name": "efh-autopilot",
  "type": "module",
  "version": "2.0.0",
  "engines": {
    "node": "20.11.1",
    "npm": ">=10.0.0"
  },
  "packageManager": "pnpm@9.7.0",
  "overrides": {
    "@sentry/react": {
      "react": "19.1.1"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@sentry/react": "^7.99.0",
    "@sentry/tracing": "^7.99.0",
    "@supabase/supabase-js": "2.57.4",
    "@vitejs/plugin-react": "4.7.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "express": "^5.1.0",
    "jsdom": "23.2.0",
    "lucide-react": "^0.545.0",
    "node-fetch": "3.3.2",
    "postcss": "^8.5.6",
    "prop-types": "15.8.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.64.0",
    "react-router-dom": "6.30.1",
    "recharts": "^3.2.1",
    "rollup-plugin-visualizer": "6.0.3",
    "slugify": "1.6.6",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "~3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "vite": "6.3.6",
    "wouter": "^3.7.1",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "happy-dom": "^20.0.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "rimraf": "5.0.10",
    "terser": "^5.44.0",
    "typescript": "^5.9.3",
    "vike": "^0.4.195",
    "vite-plugin-ssr": "^0.4.142",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "vite",
    "dev:api": "node server/index.js",
    "dev:full": "concurrently -k -n VITE,API -c green,cyan \"./scripts/start-vite.sh\" \"./scripts/start-api.sh\"",
    "autopilot": "./scripts/autopilot.sh",
    "build": "vite build && node scripts/copy-client-to-dist.mjs",
    "postbuild": "node scripts/generate-sitemap-simple.mjs && node scripts/robots.mjs",
    "build:old": "vite build && node scripts/inject-critical-content.cjs",
    "build:base": "vite build",
    "start": "echo 'This is a static site. Use npm run preview to test locally.'",
    "start:frontend": "vite preview --host 0.0.0.0 --port 8080 --strictPort",
    "preview": "vite preview --host 0.0.0.0 --port 8080 --strictPort",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "audit": "bash scripts/audit.sh",
    "audit:full": "TARGET_URL=https://elevateforhumanity.org bash scripts/audit.sh",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "verify": "pnpm typecheck && pnpm lint && pnpm build",
    "serve:keepalive": "bash scripts/dev-ensure.sh",
    "health:once": "node scripts/healthcheck.mjs --base http://localhost:8080",
    "health:loop": "bash -c 'while true; do node scripts/healthcheck.mjs --base http://localhost:8080 || true; sleep 10; done'",
    "clean:fast": "rimraf .turbo .next/cache dist build coverage .cache",
    "clean:all": "pnpm clean:fast && rimraf node_modules && pnpm store prune",
    "clean:autopilot": "node scripts/autopilot-cleanup.js",
    "health": "bash efh-healthcheck.sh",
    "validate": "node validate-wix-content.cjs",
    "dns": "node autopilot-wix-pointing.cjs",
    "dns:watch": "node autopilot-wix-pointing.cjs --watch=300",
    "blog:publish": "node wix-blog-system/push-blog.mjs",
    "blog:test": "node wix-blog-system/autopilot-blog-integration.js test",
    "blog:success": "node wix-blog-system/autopilot-blog-integration.js successStory",
    "blog:program": "node wix-blog-system/autopilot-blog-integration.js programUpdate",
    "deploy:cloudflare": "bash cloudflare-deploy.sh",
    "deploy:auto": "npm run autopilot:advanced && npm run deploy:cloudflare",
    "compliance:check": "node scripts/compliance-check.js",
    "sitemap:generate": "node scripts/generate-sitemap.cjs",
    "cf:deploy": "wrangler pages deploy dist --project-name=elevateforhumanity",
    "cf:dev": "wrangler pages dev dist",
    "postinstall": "echo 'ðŸ¤– Advanced Autopilot Ready' && echo 'Run: npm run autopilot:always'",
    "build:seo": "npm run build"
  }
}
