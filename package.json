{
  "name": "elevate-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@10",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "tsx scripts/generate-sitemap.ts",
    "preview": "vite preview --port 3000",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "sitemap": "tsx scripts/generate-sitemap.ts",
    "test": "vitest run",
    "e2e": "playwright test",
    "prepare": "husky install",
    "setup": "node -e \"JSON.parse(require('fs').readFileSync('package.json','utf8')); console.log('ok')\" && sh -c \"[ -f package-lock.json ] && npm ci || npm install\" && bash setup-and-healthcheck.sh",
    "health:local": "bash scripts/healthcheck-local.sh"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sitemap": "7.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "16.0.1",
    "@testing-library/jest-dom": "6.4.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.8",
    "playwright": "1.47.2",
    "prettier": "3.3.3",
    "tsx": "4.19.1",
    "typescript": "5.5.4",
    "vite": "5.4.8",
    "vitest": "2.0.5"
  }
}
