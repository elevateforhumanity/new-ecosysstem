{
  "name": "workspace",
  "version": "1.0.0",
  "private": true,
  "description": "Production-ready plain Vite + TypeScript skeleton",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node simple-server.cjs",
    "server": "node simple-server.cjs",
    "server:dev": "node simple-server.cjs",
    "validate": "node validate-production.mjs",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p client/tsconfig.json",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "analyze": "vite build && npx rollup-plugin-visualizer dist/stats.html",
    "env:check": "ts-node --swc src/env.ts --check",
    "security:audit": "npm audit --omit=dev",
    "security:full": "npm audit",
    "size:check": "node scripts/size-check.mjs",
    "check:prod": "node scripts/prod-check.mjs",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build && npm run size:check",
    "polish:all": "npm run format && npm run ci && npm run check:prod",
  "release:tag": "node scripts/release-tag.mjs",
  "readiness:smoke": "node -e \"const http=require('http');http.get('http://localhost:5000/api/readiness',r=>{let d='';r.on('data',c=>d+=c);r.on('end',()=>{console.log(d);});});\"",
  "readiness:report": "node scripts/readiness-report.cjs"
  },
  "prisma": {
    "seed": "node prisma/seed.cjs"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^30.1.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "compression": "^1.7.4",
    "express": "^5.1.0",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "node-cron": "^4.2.1",
    "pino": "^9.0.0",
    "pino-http": "^10.0.0",
    "prisma": "^5.22.0",
    "pdfkit": "^0.15.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.23.1",
    "stripe": "^14.0.0"
  },
  "optionalDependencies": {},
  "overrides": {},
  "resolutions": {},
  "dependenciesExtra": {},
  "_note": "Added chokidar for autopilot file watching.",
  "legal": {
    "notice": "Commercial licensing terms included in COMMERCIAL_LICENSE.md; NDA template provided."
    },
  "keywords": [],
  "homepage": ".",
  "bugs": {},
  "repository": {},
  "packageManager": "npm@10.0.0"
}
