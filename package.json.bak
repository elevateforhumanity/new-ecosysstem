{
  "name": "elevate-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18 <19"
  },
  "scripts": {
    "dev": "bash scripts/dev-start.sh",
    "build": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "lint": "eslint .",
    "test": "vitest run --environment=jsdom",
    "deploy": "bash ./deploy.sh",
    "verify:deploy": "node ./scripts/verify-deploy.mjs",
    "dev:proxy": "node scripts/dev-proxy.js",
    "dev:all": "npm-run-all -p dev dev:proxy",
    "plugins:fix": "node scripts/plugins-diagnose.cjs --apply",
    "plugins:nuke": "bash scripts/plugins-nuke.sh"
  },
  "dependencies": {
    "dotenv": "16.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.9.1",
    "sitemap": "7.1.1",
    "stripe": "16.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "10.4.20",
    "eslint": "^8.57.1",
    "http-proxy": "^1.18.1",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.47",
    "prettier": "^3.6.2",
    "tailwindcss": "3.4.14",
    "tsx": "4.19.1",
    "typescript": "5.5.4",
    "vite": "5.4.8"
  }
}
