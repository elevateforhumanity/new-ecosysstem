{
  "name": "elevate-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Elevate for Humanity web app",
  "engines": { "node": "20.x" },
  "scripts": {
    "preinstall": "npm config set legacy-peer-deps true",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0 || true",
    "format": "prettier --write .",
    "deploy": "./deploy.sh",
    "start:prod": "npx serve -s dist -l 8080"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.1",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.0",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "vite": "5.4.8",
    "serve": "14.2.3"
  },
  "overrides": {
    "eslint": "8.57.0"
  }
}
