{
  "version": 2,
  "builds": [{ "src": "index.html", "use": "@vercel/static" }],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/student-portal", "dest": "/student-portal.html" },
    { "src": "/enroll", "dest": "/student-portal.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
